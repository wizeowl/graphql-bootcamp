{
  "name": "basics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "seed": "babel-node data/createTestData.js",
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "dev": "env-cmd config/dev.env nodemon src/index.js --exec babel-node --ext js,graphql",
    "test": "env-cmd ./config/test.env jest --watch ",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env",
    "local-deploy": "env-cmd config/dev.env prisma deploy",
    "prt": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "globalSetup": "./tests/config/globalSetup.js",
    "globalTeardown": "./tests/config/globalTeardown.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "apollo-boost": "^0.1.14",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cross-fetch": "^2.2.2",
    "graphql": "^14.0.2",
    "graphql-yoga": "^1.16.7",
    "jsonwebtoken": "^8.3.0",
    "prisma-binding": "^2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "env-cmd": "^8.0.2",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "graphql-cli": "^2.16.4",
    "husky": "^3.1.0",
    "jest": "^23.5.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
